{
  "name": "coordinate-stock-graphql",
  "version": "0.0.1",
  "description": "A rewrite of Coordinate-Stock but using GraphQL",
  "main": "dist/server.js",
  "repository": "https://github.com/ySnoopyDogy/CoordinateStock-GraphQL.git",
  "author": "ySnoopyDogy <luxanna@menherabot.xyz>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "build": "tsc",
