    "@typescript-eslint/parser": "^5.1.0",
    "eslint": "^8.0.1",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.2",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.4.4",
    "yarn": "^1.22.17"
  },
  "dependencies": {
    "graphql": "^15.6.1",
    "koa": "^2.13.4",
    "koa-graphql": "^0.9.0",
    "koa-mount": "^4.0.0",
    "mongoose": "^6.0.12"
